
// This is tail part of code
struct node * createList(int n)
{
        struct node *head=NULL,*tmp=NULL,*prev;
        while(n--)
        {
                prev=tmp;
                tmp=(struct node *)malloc(sizeof(struct node));
                if(head==NULL)
                        head=tmp;
                tmp->data=rand()%128;
                tmp->next=NULL;
                if(prev!=NULL)
                        prev->next = tmp;
        }
        return head;
}

int main()
{
        struct node *head=createList(5);
        if(kthNodeFromLastOurs(head,6)==kthNodeFromLast(head,6)&&kthNodeFromLastOurs(head,3)==kthNodeFromLast(head,3))
        {
                return 0;
        }
        return -1;
}